{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://linkern.onrender.com/api/:path*"
    },
    {
      "source": "/l/:path*",
      "destination": "https://linkern.onrender.com/l/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

