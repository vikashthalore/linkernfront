{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://linkern-kappa.vercel.app/api/:path*"
    },
    {
      "source": "/l/:path*",
      "destination": "https://linkern-kappa.vercel.app/l/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

